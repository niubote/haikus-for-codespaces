//判断两个字符串，能否由第一个旋转得到
#include <stdio.h>
#include <string.h>

int is_left_move(char *str1,char *str2)//需要返回 1 或者 0 故而用int
{
    int len1=strlen(str1);
    int len2=strlen(str2);
    if(len1!=len2)
    return 0;
    strncat(str1,str1,len1);//6为字符串的长度(arr1 arr2不用输入的话，可以直接写字符串长度)
    char *ret=strstr(str1,str2);
    if(ret==NULL)
    {
        return 0;
    }
    else
    {
        return 1;
    }

}
int main()
{
    char arr1[20];
    char arr2[20];
   printf("请输入两个字符串： ");
   gets(arr1,arr2);
   
    int ret=is_left_move(arr1,arr2);
    if (ret=1)
    {
        printf("yes");
    }
    else
    {
        printf("no");
    }
    return 0;
}
